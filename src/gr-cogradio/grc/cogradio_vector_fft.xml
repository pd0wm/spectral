<?xml version="1.0"?>
<block>
  <name>vector_fft</name>
  <key>cogradio_vector_fft</key>
  <category>Cogradio</category>
  <import>import cogradio</import>
  <make>cogradio.vector_fft($vector_length)</make>
  
  <param>
    <name>Vector length</name>
    <key>vector_length</key>
    <value>100</value>
    <type>int</type>
  </param>

  <sink>
    <name>in</name>
    <type>complex</type>
    <vlen>$vector_length</vlen>
  </sink>
  <source>
    <name>out</name>
    <type>complex</type>
    <vlen>$vector_length</vlen>
  </source>
</block>
